{
  "expo": {
    "name": "CampaignHub",
    "slug": "CampaignHub",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 2,
      "package": "com.justlyjohn.campaignhub"
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    },
    "owner": "justlyjohn",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "9b814e0a-d644-47ac-a38e-1a20c5dc329b"
      }
    },
    "plugins": ["sentry-expo"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "bougbai-computers-netware",
            "project": "campaign-hub"
          }
        }
      ]
    },
    "updates": {
      "url": "https://u.expo.dev/9b814e0a-d644-47ac-a38e-1a20c5dc329b"
    }
  }
}
